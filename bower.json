{
  "name": "Vidyano",
  "private": true,
  "license": "MIT",
  "ignore": [
    "/*.*",
    "**/.*",
    "bin",
    ".vs",
    "bower_components",
    "node_modules",
    "packages",
    "Properties"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/2sky/vidyano.git"
  },
  "exportsOverride": {
    "polymer": {
      "": "*.html"
    },
    "webcomponentsjs": {
      "": "*-lite*.js"
    },
    "marked": {
      "lib": "lib/marked.js"
    },
    "marked-element": {
      "": "marked*.html"
    },
    "layout": {
      "": [ "layout.html", "_layout.scss" ]
    },
    "bignumber.js": {
      "": "*.min.js"
    },
    "sortable": {
      "": "sortable.min.js"
    },
    "d3": {
      "": "d3.min.js"
    },
    "codemirror": {
        "lib": "lib/*.*",
        "mode/css": "mode/css/*.*",
        "mode/htmlmixed": "mode/htmlmixed/*.*",
        "mode/javascript": "mode/javascript/*.*",
        "mode/markdown": "mode/markdown/*.*",
        "mode/sass": "mode/sass/*.*",
        "mode/xml": "mode/xml/*.*"
    }
  },
  "dependencies": {
    "es6-promise": "^3.0.2",
    "polymer": "polymer/polymer#1.3.1",
    "paper-ripple": "polymerelements/paper-ripple#^1.0.5",
    "marked-element": "polymerelements/marked-element#^1.0.2",
    "iron-a11y-keys": "polymerelements/iron-a11y-keys#^1.0.2",
    "iron-list": "polymerelements/iron-list#^1.2.6",
    "iron-media-query": "polymerelements/iron-media-query#^1.0.8",
    "bignumber.js": "^2.0.7",
    "moment": "^2.10.6",
    "sortable": "RubaXa/sortable#^1.2.1",
    "d3": "^3.5.6",
    "codemirror": "^5.13.2",
    "linqjs": "2sky/linqjs#3.0.3-Beta4",
    "pathjs": "2sky/pathjs#master",
    "layout": "2sky/layout#master"
  }
}
